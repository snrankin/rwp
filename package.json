    "scripts": {
        "main:debug": "webpack --stats verbose --color --progress --mode=development --name main --config ./webpack/webpack.config.js",
        "main:dev": "webpack --color --progress --mode=development --name main --config ./webpack/webpack.config.js",
        "main:prod": "webpack --color --progress --mode=production --name main --config ./webpack/webpack.config.js",
        "main:profile": "webpack --stats verbose --no-color --progress --mode=development --name main --profile --json > stats.json --config ./webpack/webpack.config.js",
        "fonts:debug": "webpack --stats verbose --color --progress --mode=development --name fonts --config ./webpack/webpack.config.js",
        "fonts:dev": "webpack --color --progress --mode=development --name fonts --config ./webpack/webpack.config.js",
        "fonts:prod": "webpack --color --progress --mode=production --name fonts --config ./webpack/webpack.config.js",
        "blocks:debug": "webpack --stats verbose --color --progress --mode=development --name blocks --config ./webpack/webpack.config.blocks.js",
        "blocks:dev": "webpack --color --progress --mode=development --name blocks --config ./webpack/webpack.config.blocks.js",
        "blocks:prod": "webpack --color --progress --mode=production --name blocks --config ./webpack/webpack.config.blocks.js",
        "watch": "webpack --color --progress --mode=development --watch --config ./webpack/webpack.config.js",
        "lint": "yarn lint:scripts && yarn lint:styles",
        "lint:scripts": "eslint './src/**/*.js'",
        "lint:styles": "stylelint --color --custom-syntax postcss-scss './src/**/*.{sass,scss,sss,less}'",
        "fix": "yarn fix:scripts && yarn fix:styles",
        "fix:scripts": "eslint --write './src/**/*.js'",
        "fix:styles": "stylelint --fix --color './src/**/*.{sass,scss,sss,less}'",
        "test": "webpack configtest ./webpack/webpack.config.js"
    },
    "engines": {
        "node": ">=14"
    },
    "files": [
        "assets/",
        "config/",
        "languages/",
        "includes/",
        "vendor/autoload.php",
        "vendor/composer/",
        "config.json",
        "readme.txt",
        "rwp.php",
        "uninstall.php",
        "package.json"
    ],
