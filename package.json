        "url": "https://www.riester.com"
    },
    "scripts": {
        "build": "npm-run-all build-group:*",
        "build-group:main": "webpack --color --progress --name main --config ./webpack/webpack.config.js",
        "build-group-dev:main": "webpack --color --progress --stats verbose --name main --config ./webpack/webpack.config.js",
        "build-group-prod:main": "webpack --color --progress --mode=production --name main --config ./webpack/webpack.config.js",
        "build-group:plugins": "webpack --color --progress --stats errors-only --name plugins --config ./webpack/webpack.config.js",
        "build-group-prod:plugins": "webpack --color --progress --mode=production --name plugins --config ./webpack/webpack.config.js",
        "build-group:vendor": "webpack --color --progress --name vendor --config ./webpack/webpack.config.js",
        "build-group-prod:vendor": "webpack --color --progress --mode=production --name vendor --config ./webpack/webpack.config.js",
        "build:elementor": "webpack --color --progress --name vendor --config-name elementor --config ./webpack/webpack.config.js",
        "build-prod:elementor": "webpack --color --progress --mode=production --name vendor --config-name elementor --config ./webpack/webpack.config.js",
        "build:debug": "webpack --color --progress --name vendor --config-name debug --config ./webpack/webpack.config.js",
        "build-prod:debug": "webpack --color --progress --mode=production --name vendor --config-name debug --config ./webpack/webpack.config.js",
        "build:acf": "webpack --color --progress --name vendor --config-name acf --config ./webpack/webpack.config.js",
        "build-prod:acf": "webpack --color --progress --mode=production --name vendor --config-name acf --config ./webpack/webpack.config.js",
        "build-group:fonts": "webpack --color --progress --name fonts --config ./webpack/webpack.config.js",
        "build-group-prod:fonts": "webpack --color --progress --mode=production --name fonts --config ./webpack/webpack.config.js",
        "build-group:blocks": "webpack --color --progress  --stats errors-warnings --name blocks --config ./webpack/webpack.config.blocks.js",
        "build-group-prod:blocks": "webpack --color --progress --mode=production --name blocks --config ./webpack/webpack.config.blocks.js",
        "watch": "webpack --color --progress --mode=development --watch --config ./webpack/webpack.config.js",
        "lint": "yarn lint:scripts && yarn lint:styles",
        "lint:scripts": "eslint './src/**/*.js'",
        "lint:styles": "stylelint --color --custom-syntax postcss-scss './src/**/*.{sass,scss,sss,less}'",
        "fix": "yarn fix:scripts && yarn fix:styles",
        "fix:scripts": "eslint --write './src/**/*.js'",
        "fix:styles": "stylelint --fix --color './src/**/*.{sass,scss,sss,less}'",
        "test": "webpack configtest ./webpack/webpack.config.js"
    },
    "engines": {
        "node": ">=14"
    },
    "files": [
        "assets/",
        "config/",
        "languages/",
        "includes/",
        "vendor/autoload.php",
        "vendor/composer/",
        "config.json",
        "readme.txt",
        "rwp.php",
        "uninstall.php",
        "package.json"
    ],
    "browserslist": "> 0.25%, not dead",
