    },
    "scripts": {
        "build": "yarn build:main && yarn build:vendor && yarn build:fonts && yarn build:blocks",
        "build:main": "webpack --color --progress --configName main --name main --config ./webpack/webpack.config.main.js",
        "build:admin": "webpack --color --progress --configName main --name admin --config ./webpack/webpack.config.main.js",
        "build:acf": "webpack --color --progress --configName main --name acf --config ./webpack/webpack.config.main.js",
        "build:vendor": "webpack --color --progress --configName vendor --config ./webpack/webpack.config.main.js",
        "build:bootstrap": "webpack --color --progress --configName vendor --name bootstrap --config ./webpack/webpack.config.main.js",
        "build:fancybox": "webpack --color --progress --configName vendor --name fancybox --config ./webpack/webpack.config.main.js",
        "build:gravity-forms": "webpack --color --progress --configName vendor --name gravity-forms --config ./webpack/webpack.config.main.js",
        "build:lazysizes": "webpack --color --progress --configName vendor --name lazysizes --config ./webpack/webpack.config.main.js",
        "build:select2": "webpack --color --progress --configName vendor --name select2 --config ./webpack/webpack.config.main.js",
        "build:tiny-slider": "webpack --color --progress --configName vendor --name tiny-slider --config ./webpack/webpack.config.main.js",
        "build:fonts": "webpack --color --progress --configName fonts --config ./webpack/webpack.config.main.js",
        "build:blocks": "webpack --color --progress --configName blocks --config ./webpack/webpack.config.blocks.js",
        "build:card": "webpack --color --progress --configName blocks --name card --config ./webpack/webpack.config.blocks.js",
        "build:column": "webpack --color --progress --configName blocks --name column --config ./webpack/webpack.config.blocks.js",
        "build:container": "webpack --color --progress --configName blocks --name container --config ./webpack/webpack.config.blocks.js",
        "build:global": "webpack --color --progress --configName blocks --name global --config ./webpack/webpack.config.blocks.js",
        "build:icon": "webpack --color --progress --configName blocks --name icon --config ./webpack/webpack.config.blocks.js",
        "build:row": "webpack --color --progress --configName blocks --name row --config ./webpack/webpack.config.blocks.js",
        "build:section": "webpack --color --progress --configName blocks --name section --config ./webpack/webpack.config.blocks.js",
        "build:toggle": "webpack --color --progress --configName blocks --name toggle --config ./webpack/webpack.config.blocks.js",
        "start": "webpack --no-stats --color --progress --watch --config webpack/webpack.config.main.js",
        "rmdist": "rimraf dist",
        "lint": "yarn lint:scripts && yarn lint:styles",
        "lint:scripts": "eslint 'src/scripts/**/*.js'",
        "lint:styles": "stylelint 'src/styles/**/*.{sass,scss,sss,less}'",
        "fix": "yarn fix:scripts && yarn fix:styles",
        "fix:scripts": "prettier-eslint --write 'src/**/*.js'",
        "fix:styles": "prettier-stylelint --write 'src/**/*.{sass,scss,sss,less}'",
        "test": "yarn lint"
    },
    "engines": {
        "node": ">= 8.0.0"
    },
    "files": [
        "assets",
        "dependencies",
        "i18n",
        "includes",
        "vendor",
        "readme.txt",
        "rwp.php"
    ],
    "browserslist": "> 0.25%, not dead",
