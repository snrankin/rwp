    "scripts": {
        "build": "npm-run-all build-group:* --scripts-prepend-node-path",
        "build-group:main": "webpack --color --progress --name main --config ./webpack/webpack.config.main.js",
        "build:public": "webpack --color --progress --name main --config-name public --config ./webpack/webpack.config.main.js",
        "build:admin": "webpack --color --progress --name main --config-name admin --config ./webpack/webpack.config.main.js",
        "build:bootstrap": "webpack --color --progress --name main --config-name bootstrap --config ./webpack/webpack.config.main.js",
        "build:app": "webpack --color --progress --name main --config-name app --config ./webpack/webpack.config.main.js",
        "build:debug": "webpack --color --progress --name main --config-name debug --config ./webpack/webpack.config.main.js",
        "build-group:vendor": "webpack --color --progress --name vendor --config ./webpack/webpack.config.main.js",
        "build:gravity-forms": "webpack --color --progress --name vendor --config-name gravity-forms --config ./webpack/webpack.config.main.js",
        "build:modernizr": "webpack --color --progress --name vendor --config-name modernizr --config ./webpack/webpack.config.main.js",
        "build:select2": "webpack --color --progress --name vendor --config-name select2 --config ./webpack/webpack.config.main.js",
        "build:wordpress": "webpack --color --progress --name vendor --config-name wordpress --config ./webpack/webpack.config.main.js",
        "build:acf": "webpack --color --progress --name vendor --config-name acf --config ./webpack/webpack.config.main.js",
        "build-group:plugins": "webpack --color --progress --name plugins --config ./webpack/webpack.config.main.js",
        "build:modal": "webpack --color --progress --name plugins --config-name modal --config ./webpack/webpack.config.main.js",
        "build:slider": "webpack --color --progress --name plugins --config-name slider --config ./webpack/webpack.config.main.js",
        "build-group:fonts": "webpack --color --progress --name fonts --config ./webpack/webpack.config.main.js",
        "build:bootstrap-icons": "webpack --color --progress --name fonts --config-name bootstrap-icons --config ./webpack/webpack.config.main.js",
        "build:font-awesome": "webpack --color --progress --name fonts --config-name font-awesome --config ./webpack/webpack.config.main.js",
        "build-group:blocks": "webpack --color --progress --name blocks --config ./webpack/webpack.config.blocks.js",
        "build:card": "webpack --color --progress --name blocks --config-name card --config ./webpack/webpack.config.blocks.js",
        "build:column": "webpack --color --progress --name blocks --config-name column --config ./webpack/webpack.config.blocks.js",
        "build:container": "webpack --color --progress --name blocks --config-name container --config ./webpack/webpack.config.blocks.js",
        "build:global": "webpack --color --progress --name blocks --config-name global --config ./webpack/webpack.config.blocks.js",
        "build:row": "webpack --color --progress --name blocks --config-name row --config ./webpack/webpack.config.blocks.js",
        "build:section": "webpack --color --progress --name blocks --config-name section --config ./webpack/webpack.config.blocks.js",
        "build:toggle": "webpack --color --progress --name blocks --config-name toggle --config ./webpack/webpack.config.blocks.js",
        "lint": "yarn lint:scripts && yarn lint:styles",
        "lint:scripts": "eslint 'src/scripts/**/*.js'",
        "lint:styles": "stylelint 'src/styles/**/*.{sass,scss,sss,less}'",
        "test": "yarn lint"
    },
    "engines": {
        "node": ">=14"
    },
    "files": [
        "assets/",
        "config/",
        "languages/",
        "includes/",
        "vendor/autoload.php",
        "vendor/composer/",
        "config.json",
        "readme.txt",
        "rwp.php",
        "uninstall.php",
        "package.json"
