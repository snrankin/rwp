    "author": {
        "name": "MIT",
        "email": "wordpress@riester.com",
        "url": "https://www.riester.com"
    },
    "release": {
        "repositoryUrl": "https://auth@bitbucket.org/riester/rwp.git",
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            ["@semantic-release/npm",{"npmPublish":false}],
            "@semantic-release/changelog",
            "@semantic-release/git"
        ],
        "branch": "master"
    },
    "scripts": {
        "build": "npm-run-all *:build",
        "build-dev": "npm-run-all *:dev",
        "build-prod": "npm-run-all *:prod",
        "main:build": "npm run main:dev && npm run main:prod",
        "main:debug": "webpack --stats verbose --color --progress --mode=development --name main --config ./webpack/webpack.config.js",
        "main:dev": "webpack --cache --color --progress --mode=development --name main --config ./webpack/webpack.config.js",
        "main:prod": "webpack --cache --color --progress --mode=production --name main --config ./webpack/webpack.config.js",
        "main:profile": "webpack --stats verbose --color --progress --mode=development --name main --profile --json > stats.json --config ./webpack/webpack.config.js",
        "fonts:build": "npm run fonts:dev && npm run fonts:prod",
        "fonts:debug": "webpack --stats verbose --color --progress --mode=development --name fonts --config ./webpack/webpack.config.js",
        "fonts:dev": "webpack --cache --color --progress --mode=development --name fonts --config ./webpack/webpack.config.js",
        "fonts:prod": "webpack --cache --color --progress --mode=production --name fonts --config ./webpack/webpack.config.js",
        "blocks:build": "npm run blocks:dev && npm run blocks:prod",
        "blocks:debug": "webpack --stats verbose --color --progress --mode=development --name blocks --config ./webpack/webpack.config.blocks.js",
        "blocks:dev": "webpack --cache --color --progress --mode=development --name blocks --config ./webpack/webpack.config.blocks.js",
        "blocks:prod": "webpack --cache --color --progress --mode=production --name blocks --config ./webpack/webpack.config.blocks.js",
        "watch": "webpack --color --progress --mode=development --watch --config ./webpack/webpack.config.js",
        "lint": "npm run lint:scripts && npm run lint:styles",
        "lint:scripts": "eslint './src/**/*.js'",
        "lint:styles": "stylelint --color './src/**/*.{sass,scss,sss,less}'",
        "fix": "npm run fix:scripts && npm run fix:styles",
        "fix:scripts": "eslint --write './src/**/*.js'",
        "fix:styles": "stylelint --fix --color './src/**/*.{sass,scss,sss,less}'",
        "test": "webpack configtest ./webpack/webpack.config.js"
    },
    "engines": {
        "node": ">=14"
    },
    "files": [
        "assets",
        "languages",
        "dependencies",
        "includes",
        "config.json",
        "readme.txt",
        "rwp.php",
        "uninstall.php"
    ],
    "browserslist": "> 0.25%, not dead",
