{
	"app": {
		"description": "RIESTER Core Plugin",
		"name": "RWP",
		"theme_color": "#e01c13",
		"url": "https://www.riester.com"
	},
	"cachebusting": "[name]-[contenthash]",
	"enabled": {
		"cache": false,
		"cachebusting": false,
		"debug": false,
		"manifest": true,
		"production": false
	},
	"entry": {
		"blocks": {
			"enabled": {
				"cache": false,
				"cachebusting": false,
				"debug": false,
				"manifest": true,
				"optimize": false,
				"production": false,
				"sourcemaps": true
			},
			"files": {
				"card": {
					"card": ["./blocks/card/index.js"]
				},
				"column": {
					"column": ["./blocks/column/index.js"]
				},
				"container": {
					"container": ["./blocks/container/index.js"]
				},
				"global": {
					"global": ["./blocks/global/index.js"]
				},
				"row": {
					"row": ["./blocks/row/index.js"]
				},
				"section": {
					"section": ["./blocks/section/index.js"]
				},
				"toggle": {
					"toggle": ["./blocks/toggle/index.js"]
				}
			}
		},
		"fonts": {
			"copy": [
				{
					"from": "../node_modules/bootstrap-icons/bootstrap-icons.svg",
					"to": "imgs/"
				},
				{
					"from": "../node_modules/@fortawesome/fontawesome-free/sprites/*",
					"to": "imgs/[name][ext]"
				}
			],
			"files": {
				"bootstrap-icons": {
					"bootstrap-icons": ["./css/vendor/bootstrap-icons.scss"]
				},
				"font-awesome": {
					"font-awesome": ["./css/vendor/font-awesome.scss"]
				}
			}
		},
		"main": {
			"files": {
				"admin": {
					"admin": ["./js/admin.js", "./css/admin.scss"]
				},
				"app": {
					"app": ["./js/app.js"]
				},
				"bootstrap": {
					"bootstrap": ["./css/bootstrap.scss", "./js/vendor/bootstrap.js"]
				},
				"debug": {
					"debug": ["./js/debug.js", "./css/debug.scss"]
				},
				"public": {
					"public": ["./js/public.js", "./css/public.scss"]
				}
			}
		},
		"plugins": {
			"files": {
				"alert": {
					"alert": ["./js/plugins/alert.js", "./css/plugins/alert.scss"]
				},
				"modal": {
					"modal": ["./js/plugins/modal.js", "./css/plugins/modal.scss"]
				},
				"offcanvas": {
					"offcanvas": ["./js/plugins/offcanvas.js", "./css/plugins/offcanvas.scss"]
				},
				"popover": {
					"popover": ["./js/plugins/popover.js", "./css/plugins/popover.scss"]
				},
				"scrollspy": {
					"scrollspy": ["./js/plugins/scrollspy.js"]
				},
				"slider": {
					"slider": ["./js/plugins/slider.js", "./css/plugins/slider.scss"]
				},
				"toast": {
					"toast": ["./js/plugins/toast.js", "./css/plugins/toast.scss"]
				},
				"tooltip": {
					"tooltip": ["./js/plugins/tooltip.js", "./css/plugins/tooltip.scss"]
				}
			}
		},
		"vendor": {
			"files": {
				"acf": {
					"acf": ["./js/vendor/acf.js", "./css/vendor/acf.scss"]
				},
				"gravity-forms": {
					"gravity-forms": ["./css/vendor/gravity-forms.scss"]
				},
				"lazysizes": {
					"lazysizes": ["./css/vendor/lazysizes.scss"]
				},
				"modernizr": {
					"modernizr": ["./js/vendor/modernizr.js"]
				},
				"qm": {
					"qm": ["./css/vendor/qm.scss"]
				},
				"reset": {
					"reset": ["./css/reset.scss"]
				},
				"select2": {
					"select2": ["./js/vendor/select2.js", "./css/vendor/select2.scss"]
				},
				"wordpress": {
					"wordpress": ["./css/vendor/wordpress.scss"]
				}
			}
		}
	},
	"fileprefix": "rwp-",
	"folders": {
		"blocks": "blocks",
		"css": "css",
		"dist": "assets",
		"fonts": "fonts",
		"images": "imgs",
		"js": "js",
		"src": "src"
	},
	"paths": {
		"dist": "./assets",
		"public": "/wp-content/plugins/rwp/assets/",
		"src": "./src"
	}
}
