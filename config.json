{
	"app": {
		"description": "RIESTER Core Plugin",
		"name": "RWP",
		"theme_color": "#e01c13",
		"url": "https://www.riester.com"
	},
	"cachebusting": "[name]-[contenthash]",
	"enabled": {
		"cache": false,
		"debug": false
	},
	"entry": {
		"blocks": {
			"files": {
				"card": {
					"card": ["./blocks/card/index.js"]
				},
				"column": {
					"column": ["./blocks/column/index.js"]
				},
				"container": {
					"container": ["./blocks/container/index.js"]
				},
				"global": {
					"global": ["./blocks/global/index.js"]
				},
				"row": {
					"row": ["./blocks/row/index.js"]
				},
				"section": {
					"section": ["./blocks/section/index.js"]
				},
				"toggle": {
					"toggle": ["./blocks/toggle/index.js"]
				}
			}
		},
		"fonts": {
			"copy": [
				{
					"from": "./imgs/*",
					"to": "imgs/[name][ext][query]"
				}
			],
			"files": {
				"bootstrap-icons": {
					"bootstrap-icons": ["vendor/bootstrap-icons.scss"]
				},
				"font-awesome": {
					"font-awesome": ["vendor/font-awesome.scss"]
				}
			}
		},
		"main": {
			"files": {
				"admin": {
					"admin": ["admin.js", "admin.scss"]
				},
				"app": {
					"app": ["util/utils.js"]
				},
				"bootstrap": {
					"bootstrap": ["bootstrap.scss", "vendor/bootstrap.js"]
				},
				"public": {
					"public": ["public.js", "public.scss"]
				}
			},
			"webpack": {
				"output": {
					"library": {
						"name": "rwp",
						"type": "assign-properties"
					}
				}
			}
		},
		"plugins": {
			"files": {
				"alert": {
					"alert": ["plugins/alert.js", "plugins/alert.scss"]
				},
				"modal": {
					"modal": ["plugins/modal.js", "plugins/modal.scss"]
				},
				"offcanvas": {
					"offcanvas": ["plugins/offcanvas.js", "plugins/offcanvas.scss"]
				},
				"popover": {
					"popover": ["plugins/popover.js", "plugins/popover.scss"]
				},
				"scrollspy": {
					"scrollspy": ["plugins/scrollspy.js"]
				},
				"slider": {
					"slider": ["plugins/slider.js", "plugins/slider.scss"]
				},
				"toast": {
					"toast": ["plugins/toast.js", "plugins/toast.scss"]
				},
				"tooltip": {
					"tooltip": ["plugins/tooltip.js", "plugins/tooltip.scss"]
				}
			}
		},
		"vendor": {
			"files": {
				"acf": {
					"acf": ["vendor/acf.scss"]
				},
				"debug": {
					"debug": ["vendor/debug.js", "vendor/debug.scss"]
				},
				"elementor": {
					"elementor": ["vendor/elementor.scss"]
				},
				"gravity-forms": {
					"gravity-forms": ["vendor/gravity-forms.scss"]
				},
				"lazysizes": {
					"lazysizes": ["vendor/lazysizes.scss"]
				},
				"modernizr": {
					"modernizr": ["vendor/modernizr.js"]
				},
				"qm": {
					"qm": ["vendor/qm.scss"]
				},
				"reset": {
					"reset": ["reset.scss"]
				},
				"select2": {
					"select2": ["vendor/select2.js", "vendor/select2.scss"]
				},
				"wordpress": {
					"wordpress": ["vendor/wordpress.scss"]
				}
			}
		}
	},
	"fileprefix": "rwp-",
	"folders": {
		"blocks": "blocks",
		"css": "css",
		"fonts": "fonts",
		"images": "imgs",
		"js": "js"
	},
	"paths": {
		"dist": "./assets",
		"favicon": "./src/imgs/rwp-icon.svg",
		"manifest": "./assets/manifest.json",
		"public": "/wp-content/plugins/rwp",
		"src": "./src"
	}
}
